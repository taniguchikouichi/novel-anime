<div class="container">
    <h1>レーベル一覧</h1>
        <table>
        <% @labels.each do |label| %>
            <tr>
                <th>レーベル名</th>
                <th><%= label.label_name %></th>
                <th><%= link_to "削除", admin_label_path(label),method: :delete %>
            </tr>
        <% end %>
        </table>

</div>
